{
  "extends": [
    "@rocketseat/eslint-config/node",
    "plugin:vitest-globals/recommended"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "no-camelcase": "off",
        "no-useless-constructor": "off"
        // "trailingComma": "none"
      }
    ]
  },
  "env": { "vitest-globals/env": true }
}
